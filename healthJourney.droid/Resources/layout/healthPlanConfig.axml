                    android:minWidth="25dp"
                    android:minHeight="25dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="1">
                    <TextView
                        android:text="Sleep Plan 3: Description of sleep plan 3"
                        android:layout_width="wrap_content"
